  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "comment.block.documentation.phpdoc.php"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#6f7e82"
      }
    },
    {
      "name": "Comments special token",
      "scope": [
        "comment entity.name.type",
        "comment variable",
        "comment variable.other"
      ],
      "settings": {
        "foreground": "#b8c0c2"
      }
    },
    {
      "name": "Constants",
      "scope": "constant",
      "settings": {
        "foreground": "#74b8fc"
      }
    },
    {
      "name": "Constants objects",
      "scope": "constant.object",
      "settings": {
        "foreground": "#99d874"
      }
    },
    {
      "name": "Escape characters",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#fcbb86"
      }
    },
    {
      "name": "Named entities",
      "scope": "entity.name",
      "settings": {
        "foreground": "#66d6fb"
      }
    },
    {
      "name": "Inherited classes & namespaces",
      "scope": [
        "entity.name.type.class",
        "entity.name.type.class.generic",
        "entity.other.inherited-class",
        "entity.name.type.namespace",
        "support.class",
        "variable.other.liquid",
        "keyword.other.type.php"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#66d6fb"
      }
    },
    {
      "name": "Function names",
