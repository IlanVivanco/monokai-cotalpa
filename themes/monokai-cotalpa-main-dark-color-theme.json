      "settings": {
        "foreground": "#66d6fb"
      }
    },
    {
      "name": "Tag attributes",
      "scope": "entity.other.attribute-name",
      "settings": {
        "fontStyle": "",
        "foreground": "#66d6fb"
      }
    },
    {
      "name": "CSS Class selectors",
      "scope": "entity.other.attribute-name.class",
      "settings": {
        "foreground": "#99d874"
      }
    },
    {
      "name": "CSS ID selectors",
