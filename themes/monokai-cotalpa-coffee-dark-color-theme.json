  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "comment.block.documentation.phpdoc.php"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#767676"
      }
    },
    {
      "name": "Comments special token",
      "scope": [
        "comment entity.name.type",
        "comment variable",
        "comment variable.other"
      ],
      "settings": {
        "foreground": "#b9b9b9"
      }
    },
    {
      "name": "Constants",
      "scope": "constant",
      "settings": {
        "foreground": "#7ac2ff"
      }
    },
    {
      "name": "Constants objects",
      "scope": "constant.object",
      "settings": {
        "foreground": "#a1e37b"
      }
    },
    {
      "name": "Escape characters",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#ffc48c"
      }
    },
    {
      "name": "Named entities",
      "scope": "entity.name",
      "settings": {
        "foreground": "#69e1ff"
      }
    },
    {
      "name": "Inherited classes & namespaces",
