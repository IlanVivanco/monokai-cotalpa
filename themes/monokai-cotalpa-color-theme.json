  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "comment.block.documentation.phpdoc.php"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#797366"
      }
    },
    {
      "name": "Comments special token",
      "scope": [
        "comment entity.name.type",
        "comment variable",
        "comment variable.other"
      ],
      "settings": {
        "foreground": "#b8b5ae"
      }
    },
    {
      "name": "Constants",
      "scope": "constant",
      "settings": {
        "foreground": "#74b8fc"
      }
    },
    {
      "name": "Constants objects",
      "scope": "constant.object",
      "settings": {
        "foreground": "#99d874"
      }
    },
    {
      "name": "Escape characters",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#74b8fc"
      }
    },
    {
      "name": "Named entities",
      "scope": "entity.name",
      "settings": {
        "foreground": "#66d6fb"
      }
    },
    {
      "name": "Inherited classes & namespaces",
